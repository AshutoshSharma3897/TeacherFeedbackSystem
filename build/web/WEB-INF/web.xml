<?xml version="1.0" encoding="UTF-8"?>
<web-app version="3.1" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd">
    <servlet>
        <servlet-name>Register</servlet-name>
        <servlet-class>Register</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>ServerLogin</servlet-name>
        <servlet-class>ServerLogin</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>ServerUpload</servlet-name>
        <servlet-class>ServerUpload</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>ServerFeedback</servlet-name>
        <servlet-class>ServerFeedback</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>ServerMain</servlet-name>
        <servlet-class>ServerMain</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>ShowFeedback</servlet-name>
        <servlet-class>ShowFeedback</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>ShowTimetable</servlet-name>
        <servlet-class>ShowTimetable</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>Attendance</servlet-name>
        <servlet-class>Attendance</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>Logout</servlet-name>
        <servlet-class>Login.Logout</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>Register</servlet-name>
        <url-pattern>/Register</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>ServerLogin</servlet-name>
        <url-pattern>/ServerLogin</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>ServerUpload</servlet-name>
        <url-pattern>/ServerUpload</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>ServerFeedback</servlet-name>
        <url-pattern>/ServerFeedback</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>ServerMain</servlet-name>
        <url-pattern>/ServerMain</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>ShowFeedback</servlet-name>
        <url-pattern>/ShowFeedback</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>ShowTimetable</servlet-name>
        <url-pattern>/ShowTimetable</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>Attendance</servlet-name>
        <url-pattern>/Attendance</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>Logout</servlet-name>
        <url-pattern>/Logout</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
</web-app>
